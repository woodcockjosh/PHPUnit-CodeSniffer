<?xml version="1.0"?>
<ruleset name="EtsyStandard">
 <description>The Etsy coding standard for testing.</description>
 <rule ref="Generic.CodeAnalysis.UselessOverridingMethod" />
 <rule ref="Generic.Metrics.CyclomaticComplexity">
  <properties>
   <property name="absoluteComplexity" value="2" />
   <property name="complexity" value="2" />
  </properties>
 </rule>
 <rule ref="Generic.Metrics.NestingLevel">
  <properties>
   <property name="absoluteNestingLevel" value="1" />
   <property name="nestingLevel" value="1" />
  </properties>
 </rule>
</ruleset>
